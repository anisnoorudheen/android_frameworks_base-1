<animated-selector xmlns:android="http://schemas.android.com/apk/res/android"
    android:constantSize="true">
    <item
        android:id="@+id/off"
        android:bottom="1.5dp"
        android:left="27.5dp"
        android:right="2dp"
        android:state_enabled="false"
        android:top="1.5dp">
        <shape
            android:shape="oval"
            android:tint="?attr/colorSwitchThumbNormal"
            android:tintMode="multiply">
            <solid android:color="@color/white" />
            <stroke
                android:width="2dp"
                android:color="#0000ffff" />
            <size
                android:width="30dp"
                android:height="30dp" />
        </shape>
    </item>
    <item
        android:id="@+id/on"
        android:bottom="1.5dp"
        android:left="27.5dp"
        android:right="2dp"
        android:state_checked="true"
        android:top="1.5dp">
        <shape
            android:shape="oval"
            android:tint="?attr/colorControlActivated"
            android:tintMode="multiply">
            <solid android:color="@color/white" />
            <stroke
                android:width="2dp"
                android:color="#0000ffff" />
            <size
                android:width="30dp"
                android:height="30dp" />
        </shape>
    </item>
    <item
        android:id="@+id/off"
        android:bottom="1.5dp"
        android:left="27.5dp"
        android:right="2dp"
        android:top="1.5dp">
        <shape
            android:shape="oval"
            android:tint="?attr/colorSwitchThumbNormal"
            android:tintMode="multiply">
            <solid android:color="@color/white" />
            <stroke
                android:width="2dp"
                android:color="#0000ffff" />
            <size
                android:width="30dp"
                android:height="30dp" />
        </shape>
    </item>
    <transition
        android:fromId="@+id/off"
        android:toId="@+id/on">
        <animation-list>
            <item
                android:bottom="1.5dp"
                android:duration="15"
                android:left="27.5dp"
                android:right="2dp"
                android:top="1.5dp">
                <shape
                    android:shape="oval"
                    android:tint="?attr/colorSwitchThumbNormal"
                    android:tintMode="multiply">
                    <solid android:color="@color/white" />
                    <stroke
                        android:width="2dp"
                        android:color="#0000ffff" />
                    <size
                        android:width="30dp"
                        android:height="30dp" />
                </shape>
            </item>
            <item
                android:bottom="1.5dp"
                android:duration="15"
                android:left="27.5dp"
                android:right="2dp"
                android:top="1.5dp">
                <shape
                    android:shape="oval"
                    android:tint="?attr/colorControlActivated"
                    android:tintMode="multiply">
                    <solid android:color="@color/white" />
                    <stroke
                        android:width="2dp"
                        android:color="#0000ffff" />
                    <size
                        android:width="30dp"
                        android:height="30dp" />
                </shape>
            </item>
        </animation-list>
    </transition>
    <transition
        android:fromId="@+id/on"
        android:toId="@+id/off">
        <animation-list>
            <item
                android:bottom="1.5dp"
                android:duration="15"
                android:left="27.5dp"
                android:right="2dp"
                android:top="1.5dp">
                <shape
                    android:shape="oval"
                    android:tint="?attr/colorControlActivated"
                    android:tintMode="multiply">
                    <solid android:color="@color/white" />
                    <stroke
                        android:width="2dp"
                        android:color="#0000ffff" />
                    <size
                        android:width="30dp"
                        android:height="30dp" />
                </shape>
            </item>
            <item
                android:bottom="1.5dp"
                android:duration="15"
                android:left="27.5dp"
                android:right="2dp"
                android:top="1.5dp">
                <shape
                    android:shape="oval"
                    android:tint="?attr/colorSwitchThumbNormal"
                    android:tintMode="multiply">
                    <solid android:color="@color/white" />
                    <stroke
                        android:width="2dp"
                        android:color="#0000ffff" />
                    <size
                        android:width="30dp"
                        android:height="30dp" />
                </shape>
            </item>
        </animation-list>
    </transition>
</animated-selector>
